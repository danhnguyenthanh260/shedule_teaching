{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_GOOGLE_CLIENT_ID": "@google_client_id"
  },
  "headers": [
    {
      "source": "/callback.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
